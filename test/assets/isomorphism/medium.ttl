@prefix ex: <http://example.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:person1 ex:name "Alice" .
ex:person1 ex:age "25"^^xsd:int .
ex:person1 ex:city "New York" .
ex:person2 ex:name "Bob" .
ex:person2 ex:age "30"^^xsd:int .
