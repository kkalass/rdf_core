# Bibliographic resources with Dublin Core metadata
ex:publication1 a bibo:Book .
ex:publication1 dc:title "RDF and Semantic Web Technologies"@en .
ex:publication1 dc:title "RDF et les Technologies du Web Sémantique"@fr .
ex:publication1 dc:creator ex:personA .
ex:publication1 dc:creator ex:personB .
ex:publication1 dc:publisher "Academic Press"@en .
ex:publication1 dc:date "2023-05-15"^^xsd:date .
ex:publication1 dc:language "en" .
ex:publication1 dc:subject ex:conceptX .
ex:publication1 dc:subject ex:conceptY .
ex:publication1 bibo:pages "450"^^xsd:int .
ex:publication1 bibo:isbn "978-0123456789"^^xsd:string .

ex:publication2 a bibo:Article .
ex:publication2 dc:title "Ontology Engineering Best Practices"@en .
ex:publication2 dc:title "Bewährte Praktiken der Ontologie-Entwicklung"@de .
ex:publication2 dc:creator ex:personC .
ex:publication2 dcterms:isPartOf ex:journal1 .
ex:publication2 dc:date "2023-03-10"^^xsd:date .
ex:publication2 dc:language "en" .
ex:publication2 dc:subject ex:conceptZ .

ex:journal1 a bibo:Journal .
ex:journal1 dc:title "Journal of Semantic Technologies"@en .
ex:journal1 bibo:issn "1234-5678"^^xsd:string .

# SKOS Concept Scheme and Concepts
ex:taxonomy1 a skos:ConceptScheme .
ex:taxonomy1 dc:title "Web Technologies Taxonomy"@en .
ex:taxonomy1 dc:creator ex:personA .

ex:conceptX a skos:Concept .
ex:conceptX skos:inScheme ex:taxonomy1 .
ex:conceptX skos:prefLabel "Semantic Web"@en .
ex:conceptX skos:prefLabel "Web Semántico"@es .
ex:conceptX skos:prefLabel "Web Sémantique"@fr .
ex:conceptX skos:definition "An extension of the World Wide Web through standards by the World Wide Web Consortium"@en .
ex:conceptX skos:broader ex:conceptW .
ex:conceptX skos:narrower ex:conceptY .
ex:conceptX skos:narrower ex:conceptZ .

ex:conceptY a skos:Concept .
ex:conceptY skos:inScheme ex:taxonomy1 .
ex:conceptY skos:prefLabel "Linked Data"@en .
ex:conceptY skos:prefLabel "Données Liées"@fr .
ex:conceptY skos:broader ex:conceptX .

ex:conceptZ a skos:Concept .
ex:conceptZ skos:inScheme ex:taxonomy1 .
ex:conceptZ skos:prefLabel "Ontology Engineering"@en .
ex:conceptZ skos:prefLabel "Ingeniería de Ontologías"@es .
ex:conceptZ skos:broader ex:conceptX .

ex:conceptW a skos:Concept .
ex:conceptW skos:inScheme ex:taxonomy1 .
ex:conceptW skos:prefLabel "Web Technologies"@en .
ex:conceptW skos:topConceptOf ex:taxonomy1 .

# Time-based events and intervals
ex:event1 a time:Interval .
ex:event1 dc:title "Semantic Web Conference 2023"@en .
ex:event1 time:hasBeginning _:timeStart .
ex:event1 time:hasEnd _:timeEnd .
_:timeStart time:inXSDDateTime "2023-10-15T09:00:00Z"^^xsd:dateTime .
_:timeEnd time:inXSDDateTime "2023-10-17T18:00:00Z"^^xsd:dateTime .
ex:event1 dc:location "Vienna, Austria"@en .

ex:moment1 a time:Instant .
ex:moment1 time:inXSDDateTime "2023-10-16T14:30:00Z"^^xsd:dateTime .
ex:moment1 dc:title "RDF Graph Isomorphism in Practice"@en .
ex:moment1 dc:creator ex:personA .
ex:moment1 time:intervalDuring ex:event1 .